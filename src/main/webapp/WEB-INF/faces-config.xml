<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd"
    version="2.1">
	<application>
		<message-bundle>l18n.web.common.validator.messages</message-bundle>
		<resource-bundle>
			<base-name>l18n.web.text.home</base-name>
			<var>text_home</var>
		</resource-bundle>
		<resource-bundle>
			<base-name>l18n.web.text.common</base-name>
			<var>text_common</var>
		</resource-bundle>
		<resource-bundle>
			<base-name>l18n.web.text.profile</base-name>
			<var>text_profile</var>
		</resource-bundle>
	</application>		
</faces-config>
